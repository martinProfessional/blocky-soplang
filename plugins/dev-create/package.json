{
  "name": "@blockly/create-package",
  "version": "2.0.0",
  "description": "A tool for creating a Blockly plugin based on a template.",
  "bin": "./bin/create.js",
  "author": "Blockly Team",
  "keywords": [
    "blockly",
    "dev",
    "create",
    "package",
    "plugin",
    "template"
  ],
  "files": [
    "*",
    "templates/sample-app/*"
  ],
  "scripts": {
    "prepack": "cp -r ../../examples/sample-app ./templates && cp -r ../../examples/sample-app-ts ./templates"
  },
  "homepage": "https://github.com/google/blockly-samples/tree/master/plugins/dev-create#readme",
  "bugs": {
    "url": "https://github.com/google/blockly-samples/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/blockly-samples.git",
    "directory": "plugins/dev-create"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^10.0.0",
    "fs-extra": "^9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "eslintConfig": {
    "extends": "@blockly/eslint-config"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "blockly": "^10.0.0"
  }
}
